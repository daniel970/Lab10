package edu.handong.csee.java;

public class Salesman {
	private String Name;
	public String getName() {
		return Name;
	}
	public void setName(String name) {
		Name = name;
	}
	public double getSales() {
		return Sales;
	}
	public void setSales(double sales) {
		Sales = sales;
	}
	private double Sales;
	
}
